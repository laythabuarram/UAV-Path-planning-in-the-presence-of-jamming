  author={Senhaji-Mouhaddib, Issam and Bouzakri, Samir and Lagrat, Ismail},
  booktitle={2024 10th International Conference on Optimization and Applications (ICOA)}, 
  title={A Foundational Framework for an UAV GPS Jamming Simulator}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={This paper presents a theoretical framework for a GPS signals jamming simulator designed to evaluate the effects of jamming on a GPS receiver onboard a UAV in an aerodynamic environment. This methodological study focuses on modeling the jammer, the GPS receiver, the propagation environment, and the electronic attack scenario. While commercially available GNSS jamming simulators exist, they can be expensive due to their complexity. This proposed simulator offers an alternative approach to assess the reliability of GPS observables like pseudo ranges, carrier phases, and Doppler measurements under jamming attacks. Furthermore, the framework allows for the exploration and development of anti-jamming techniques capable of detection, mitigation, and localization of interference sources.},
  keywords={Location awareness;Global navigation satellite system;Prevention and mitigation;Receivers;Interference;Autonomous aerial vehicles;Reliability;Jamming;Optimization;Global Positioning System;GPS;Jamming;Modeling;Simulation;Navigation;Aeronautics},
  doi={10.1109/ICOA62581.2024.10754331},
  ISSN={2768-6388},
  month={Oct},}@INPROCEEDINGS{10976907,
  author={Zhu, Chen and Lee, Young-Hee and Triolo, Antonino and Meurer, Michael},
