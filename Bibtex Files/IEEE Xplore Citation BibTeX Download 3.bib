  journal={IEEE Transactions on Vehicular Technology}, 
  title={UAV Trajectory Optimisation in Smart Cities Using Modified A* Algorithm Combined With Haversine and Vincenty Formulas}, 
  year={2023},
  volume={72},
  number={8},
  pages={9757-9769},
  abstract={It is anticipated that the backbone of Smart Cities concerning automation and networking will be formed by Unmanned Aerial Vehicles in the imminent future. Therefore, our research focuses on developing advanced microcontrollers embedded with Artificial Intelligence techniques for self-governing Unmanned Aerial Vehicles. The main objective of this research was to enable full automation for the execution of flight paths with non-trivial sequences that will be performed with centimetre-level accuracy. Also, by utilising dynamic flight plans and trajectories, we aim to secure autonomous aviation based on norms, with control loops and fundamental constraints. More specifically, we evolved a novel algorithmic technique for trajectory optimisation, which deploys a modification to the A* search algorithm, implemented by the Haversine formula and enhances accuracy using Vincenty's formula. Furthermore, realistic values for trajectory optimisation and obstacle avoidance were found through the implementation of a simulative investigation. The outcomes of our methodology indicate that the safety constraints associated with the integration of Unmanned Aerial Vehicles in the urban environment can be significantly mitigated. Consequently, their effectiveness will be increased in realising their diverse operations and capabilities.},
  keywords={Trajectory;Transportation;Automobiles;Autonomous aerial vehicles;Smart cities;Safety;Optimization;Unmanned aerial vehicles;air traffic control;air transportation;Smart vehicles;aircraft navigation;Internet of Things;autonomous},
  doi={10.1109/TVT.2023.3254604},
  ISSN={1939-9359},
  month={Aug},}@INPROCEEDINGS{10551188,
  author={Yan, Xiao and Wang, Qingping and Wang, Yanqi and Wang, Chao},
  booktitle={2023 4th International Conference on Computer, Big Data and Artificial Intelligence (ICCBD+AI)}, 
  title={Distributed cooperative jamming task allocation algorithm based on improved multi-pheromone ant colony algorithm}, 
  year={2023},
  volume={},
  number={},
  pages={305-312},
  abstract={In order to solve the problem of distributed cooperative jamming task allocation of UAV swarm, an optimal allocation method based on improving the cover efficiency of manned aircraft is proposed. Firstly, based on the cooperative interference of multiple UAVs, a model of common detection exposure area of networked radar is established.Then, according to the concept of safety zone in the jamming effectiveness evaluation criterion, a more objective and comprehensive jamming evaluation function model is established by combining the shortest distance between the manned aircraft and the exposed area boundary, the feasible area range for the manned aircraft to plan the safe track in the next step and the degree of suppression of the radar exposed area. Finally, based on the multi-pheromone ant colony algorithm, the adjustment and improvement are made to adjust the pheromone learning rate and path planning strategy more intelligently, improve the model optimization rate and optimization ability, and give the specific implementation logic. Through simulation, it is found that this method has more stable and fast convergence ability than traditional genetic algorithm, and has stronger global optimization ability than traditional ant colony algorithm. Especially With the expansion of operation scale, the convergence performance of the algorithm is more prominent, and it has better real-time operation and can be closer to practical application.},
  keywords={Heuristic algorithms;Atmospheric modeling;Radar tracking;Resource management;Task analysis;Jamming;Aircraft;Multi-pheromone ant colony algorithm;adaptive learning rate;joint task;task allocation},
  doi={10.1109/ICCBD-AI62252.2023.00058},
  ISSN={},
  month={Dec},}@ARTICLE{10234408,
  author={Deng, Min and Yao, Zhiqiang and Li, Xingwang and Wang, Han and Nallanathan, Arumugam and Zhang, Zeyang},
  journal={IEEE Journal on Selected Areas in Communications}, 
  title={Dynamic Multi-Objective AWPSO in DT-Assisted UAV Cooperative Task Assignment}, 
  year={2023},
  volume={41},
  number={11},
  pages={3444-3460},
  abstract={In recent years, more and more attention has been paid to the unmanned aerial vehicle (UAV) cooperative task assignment. In order to complete the task with the lowest cost, some researchers use multi-objective optimization to solve the assignment problem. But few of them consider the complex dynamic scenarios. In this article, the time-varying resource supply and demands are provided by established digital twins (DTs) of UAVs and targets, thereby enabling accurate decision guidance for dynamic task assignment. It takes the scheduling cost, path cost, risk cost and total task time cost as the optimization objectives. To solve this model, an improved dynamic multi-objective adaptive weighted particle swarm Optimization algorithm (DMOAWPSO) is proposed. In the initialization stage, a heuristic method is used to increase the effectiveness of the solution. Besides, the adaptive mutation and subgroup methods are adopted to improve the diversity of the solution. Then, effective environment change detection and response strategies are designed to adapt to dynamic scenarios. Finally, the evaluation metrics are calculated in different instances. Compared with the popular and classic dynamic multi-objective algorithms, the simulation results verify that the proposed algorithm is effective and can cope with the environment changes better in solving the task assignment problem.},
  keywords={Task analysis;Heuristic algorithms;Jamming;Autonomous aerial vehicles;Weapons;Costs;Change detection algorithms;Dynamic multi-objective optimization;UAV;digital twin;task assignment;evolutionary algorithm},
